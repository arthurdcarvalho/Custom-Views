<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CompostCustomView">

        <attr name="hasSendButton" format="boolean"/>
        <attr name="sendText" format="string" />

        <attr name="maskType" format="enum">
            <enum name="CPF" value="1"/>
            <enum name="CEP" value="2"/>
            <enum name="CellPhone" value="3"/>
            <enum name="Phone" value="4"/>
            <enum name="CreditCard" value="5"/>
            <enum name="CreditCardExpiration" value="6"/>
            <enum name="Date" value="7"/>
        </attr>


    </declare-styleable>

</resources>